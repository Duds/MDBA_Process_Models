      <bpmndi:BPMNShape id="Shape_Lane_QualityTeam" bpmnElement="Lane_QualityTeam" isHorizontal="true">
        <dc:Bounds x="140" y="420" width="1600" height="120" />
      </bpmndi:BPMNShape>
    </bpmndi:BPMNPlane>
  </bpmndi:BPMNDiagram>
</bpmn:definitions> 